    // TAILWIND
    {
      "name": "Tailwind: punctuation",
      "scope": ["punctuation.definition.keyword.tailwind"],
      "settings": {
        "foreground": "#FA198B"
      }
    },
    {
      "name": "CSS: Entities",
      "scope": ["keyword.control.at-rule.apply.tailwind"],
      "settings": {
        "fontStyle": "",
        "foreground": "#9F84BD"
      }
    }
  ]
}
