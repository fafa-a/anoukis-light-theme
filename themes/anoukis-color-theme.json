{
  "name": "anoukis",
  "type": "light",
  "colors": {
    "editor.background": "#F2F4F6",
    "editor.foreground": "#0c1618"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#B4B4C1"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#B4B4C1"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": {
        "foreground": "#448C27"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#660000"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#4B83CD"
      }
    },
    {
      "name": "Types",
      "scope": ["storage.type", "support.type"],
      "settings": {
        "foreground": "#7A3E9D"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#AB6526"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#7A3E9D"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "fontStyle": "",
        "foreground": "#FF3336"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#7A3E9D"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#660000"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": ["constant.numeric", "constant.character", "constant"],
      "settings": {
        "foreground": "#AB6526"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#00C6B2"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#4B83CD"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#AB6526"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#777777"
      }
    },
    // HTML
    {
      "name": "HTML: Doctype Declaration",
      "scope": [
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#AAAAAA"
      }
    },
    {
      "name": "HTML: Tags",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FA198B"
      }
    },
    {
      "name": "HTML: Tags",
      "scope": ["meta.tag"],
      "settings": {
        "foreground": "#FA198B"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": [
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7E5A9B"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": ["text.html.derivative"],
      "settings": {
        "foreground": "#394855"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": ["string.quoted.double.html", "string.quoted.single.html"],
      "settings": {
        "foreground": "#00C6B2"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": [
        "punctuation.separator.key-value.html",
        "punctuation.definition.string.begin.html",
        "punctuation.definition.string.end.html"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFACE4"
      }
    },
    // CSS
    {
      "name": "CSS: Selectors",
      "scope": [
        "meta.selector",
        "meta.selector entity",
        "meta.selector entity punctuation"
      ],
      "settings": {
        "foreground": "#00C6B2"
      }
    },
    {
      "name": "CSS: Selectors",
      "scope": ["entity.name.tag.css", "keyword.operator.combinator.css"],
      "settings": {
        "foreground": "#FA198B"
      }
    },

    {
      "name": "CSS: Property Names",
      "scope": [
        "meta.property-name",
        "support.type.property-name",
        "constant.numeric.css"
      ],
      "settings": {
        "foreground": "#EEA243"
      }
    },
    {
      "name": "CSS: Property Names",
      "scope": "support.constant.color.w3c-standard-color-name.css",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EEA243"
      }
    },
    {
      "name": "CSS: Property Values",
      "scope": [
        "meta.property-value constant.other",
        "support.constant.property-value",
        "support.constant.color.w3c-extended-color-name.css",
        "support.function.gradient.css",
        "support.function.misc.css",
        "support.function.url.css",
        "support.constant.vendored.property-value.css",
        "invalid.deprecated.gradient.function.css",
        "meta.function.gradient.invalid.deprecated.gradient.css",
        "support.constant.font-name.css",
        "support.function.misc.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF7477"
      }
    },
    {
      "name": "CSS: Property Values",
      "scope": "constant.other.color.rgb-value.hex.css",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF7477"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.section.property-list.begin.bracket.curly.css",
        "punctuation.section.property-list.end.bracket.curly.css",
        "meta.function.variable.css",
        "punctuation.section.function.begin.bracket.round.css",
        "punctuation.section.function.end.bracket.round.css",
        "punctuation.section.begin.bracket.curly.css",
        "punctuation.section.end.bracket.curly.css",
        "punctuation.definition.string.begin.css",
        "punctuation.definition.string.end.css"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FF3336"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["meta.property-value.css", "support.constant.font-name.css"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF3336"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.keyword.css",
        "punctuation.terminator.rule.css",
        "punctuation.terminator.rule.css",
        "punctuation.separator.key-value.css",
        "punctuation.definition.string.begin.css",
        "punctuation.definition.string.end.css"
      ],
      "settings": {
        "foreground": "#FF3336"
      }
    },
    {
      "name": "CSS: Entities",
      "scope": ["string.quoted.single.css", "string.quoted.double.css"],
      "settings": {
        "fontStyle": "",
        "foreground": "#306B34"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.keyword.css",
        "punctuation.terminator.rule.css"
      ],
      "settings": {
        "foreground": "#FF3336"
      }
    },
    {
      "name": "CSS: Important Keyword",
      "scope": [
        "keyword.other.important",
        "keyword.other.unit.px.css",
        "keyword.other.unit.rem.css",
        "keyword.other.unit.em.css",
        "keyword.other.unit.percentage.css",
        "keyword.other.unit.vw.css",
        "keyword.other.unit.vh.css",
        "keyword.other.unit.vmax.css",
        "keyword.other.unit.vmin.css",
        "keyword.other.unit.ch.css",
        "keyword.other.unit.cm.css",
        "keyword.other.unit.pc.css",
        "keyword.other.unit.ex.css",
        "keyword.other.unit.fr.css",
        "keyword.other.unit.in.css",
        "keyword.other.unit.mm.css",
        "keyword.other.unit.pt.css",
        "keyword.other.unit.deg.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#306B34"
      }
    },
    {
      "name": "CSS: Entities",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "punctuation.definition.entity.css",
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.class.css.postcss",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00C6B2"
      }
    },
    {
      "name": "CSS: Entities",
      "scope": ["variable.css", "variable.argument.css"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#004777"
      }
    },
    {
      "name": "CSS: Entities",
      "scope": [
        "keyword.control.at-rule.css",
        "keyword.control.at-rule.font-face.css",
        "keyword.control.at-rule.media.css",
        "keyword.control.at-rule.keyframes.css",
        "keyword.control.at-rule.viewport.css",
        "keyword.control.at-rule.counter-style.css"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#9F84BD"
      }
    },
    {
      "name": "CSS: Entities",
      "scope": ["meta.at-rule.header.css", "support.constant.media.css"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#B6A2CD"
      }
    },
    //  MARKDOWN
    {
      "name": "punctuation",
      "scope": [
        "punctuation.definition.heading.markdown",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.string.markdown",
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#FA198B"
      }
    },
    {
      "name": "punctuation",
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FA198B"
      }
    },
    {
      "name": "punctuation",
      "scope": [
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.quote.begin.markdown",
        "punctuation.definition.raw.markdown",
        "markup.heading.setext.2.markdown"
      ],
      "settings": {
        "foreground": "#48392A"
      }
    },
    {
      "name": "Markup: Entities",
      "scope": "entity.name.section.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7E5A9B"
      }
    },
    {
      "name": "Mardown: Entities",
      "scope": "string.other.link.description.markdown",
      "settings": {
        "foreground": "#00C6B2"
      }
    },
    {
      "name": "Mardown: Entities",
      "scope": "markup.underline.link.image.markdown",
      "settings": {
        "foreground": "#f7cb15"
      }
    },
    {
      "name": "Mardown: Entities",
      "scope": [
        "text.html.markdown",
        "markup.list.numbered.markdown",
        "markup.list.unnumbered.markdown"
      ],
      "settings": {
        "foreground": "#449DD1"
      }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": ["markup.italic", "markup.italic.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#449DD1"
      }
    },
    {
      "name": "Markup: Error",
      "scope": "markup.error",
      "settings": {
        "foreground": "#660000"
      }
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markup: Link",
      "scope": "meta.link",
      "settings": {
        "foreground": "#4B83CD"
      }
    },
    {
      "name": "Markup: Output",
      "scope": ["markup.output", "markup.raw"],
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Markup: Prompt",
      "scope": "markup.prompt",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#306B34"
      }
    },
    {
      "name": "Markup: Strong",
      "scope": ["markup.bold", "markup.bold.markdown"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#449DD1"
      }
    },
    {
      "name": "Markup: Traceback",
      "scope": "markup.traceback",
      "settings": {
        "foreground": "#660000"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Quote",
      "scope": ["markup.quote", "markup.quote.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7A3E9D"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#4B83CD"
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#00C6B2"
      }
    },
    {
      "name": "Markup Inline",
      "scope": ["markup.inline.raw", "markup.inline.raw.string.markdown"],
      "settings": {
        "fontStyle": "",
        "foreground": "#EEA243"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": "meta.diff.header.from-file",
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": "meta.diff.header.to-file",
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "JS: keyword",
      "scope": [
        "keyword.control.import.js",
        "keyword.control.from.js",
        "meta.import.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#9F84BD"
      }
    },
    {
      "name": "JS: variable",
      "scope": [
        "variable.other.readwrite.alias.js",
        "variable.other.readwrite.js",
        "variable.other.constant.js",
        "variable.other.object.js",
        "variable.other.property.js"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#0077b6"
      }
    },
    {
      "name": "JS: entities",
      "scope": ["sentity.name.function.js", "meta.function-call.js"],
      "settings": {
        "fontStyle": "",
        "foreground": "#861657"
      }
    },
    {
      "name": "JS: storage",
      "scope": ["storage.type.js", "support.class.builtin.js"],
      "settings": {
        "fontStyle": "",
        "foreground": "#03045E"
      }
    },
    {
      "name": "JS: support",
      "scope": ["support.class.builtin.js"],
      "settings": {
        "fontStyle": "",
        "foreground": "#03045E"
      }
    },
    {
      "name": "JS: meta",
      "scope": ["meta.object-literal.key.js"],
      "settings": {
        "fontStyle": "",
        "foreground": "#9F84BD"
      }
    },
    {
      "name": "JS: constant",
      "scope": [
        "constant.language.boolean.false.js",
        "constant.language.boolean.true.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#fb8500"
      }
    },
    {
      "name": "JS: punctuation",
      "scope": [
        "punctuation.definition.block.js",
        "meta.block.js",
        "punctuation.terminator.statement.js",
        "punctuation.definition.parameters.begin.js",
        "punctuation.definition.parameters.end.js"
      ],
      "settings": {
        "foreground": "#FF3336"
      }
    },
    {
      "name": "JS: punctuation",
      "scope": [
        "punctuation.definition.string.begin.js",
        "punctuation.definition.string.end.js"
      ],
      "settings": {
        "foreground": "#FFACE4"
      }
    },
    {
      "name": "JS: punctuation",
      "scope": ["meta.brace.round.js"],
      "settings": {
        "foreground": "#9F84BD"
      }
    },
    // VUEJS
    {
      "name": "Vue: entities",
      "scope": [
        "entity.name.tag.template.html",
        "entity.name.tag.script.html",
        "entity.name.tag.style.html"
      ],
      "settings": {
        "foreground": "#FA198B"
      }
    },
    {
      "name": "Vue: punctuation",
      "scope": [
        "punctuation.definition.generic.begin.html",
        "punctuation.definition.generic.end.html"
